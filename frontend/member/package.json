{
  "name": "pfm-member-portal",
  "version": "1.0.0",
  "description": "Member Portal for PFM Community Management Application",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:container": "next dev -p 3002 -H 0.0.0.0",
    "build": "next build",
    "start": "next start -p 3002",
    "start:container": "NODE_ENV=production next start -p 3002 -H 0.0.0.0",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=\"(unit|components)\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:e2e": "jest --testPathPattern=\"e2e\"",
    "test:services": "jest --testPathPattern=\"services\"",
    "ci:test": "npm run lint && npm run type-check && npm run test:ci",
    "ci:build": "npm run build",
    "health-check": "node scripts/health-check.js",
    "container:status": "docker ps --filter name=pfm"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@project-serum/anchor": "^0.26.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.95.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-intersection-observer": "^9.5.3",
    "react-query": "^3.39.3",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.31",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "url": "^0.11.4"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11"
  },
  "keywords": [
    "member",
    "community",
    "voting",
    "solana",
    "web3",
    "react",
    "nextjs",
    "docker",
    "containerized"
  ],
  "author": "PFM Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "container": {
    "port": 3002,
    "environment": "development",
    "healthCheck": "/api/health",
    "dependencies": [
      "backend",
      "postgres",
      "redis",
      "solana-local-validator"
    ]
  }
}
